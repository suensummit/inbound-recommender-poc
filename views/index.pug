			var book_num_2 = book_num_list[1];
			var book_num_3 = book_num_list[2];
			var book_num_4 = book_num_list[3];
			$('#book_1').attr('src', book_list[book_num_1].img).wrap('<a href="' + book_list[book_num_1].url + '" target="_blank" </a>')
			$('#book_1_title').text(book_list[book_num_1].title + '')
			$('#book_2').attr('src', book_list[book_num_2].img).wrap('<a href="' + book_list[book_num_2].url + '" target="_blank" </a>')
			$('#book_2_title').text(book_list[book_num_2].title + '')
			$('#book_3').attr('src', book_list[book_num_3].img).wrap('<a href="' + book_list[book_num_3].url + '" target="_blank" </a>')
			$('#book_3_title').text(book_list[book_num_3].title + '')
			$('#book_4').attr('src', book_list[book_num_4].img).wrap('<a href="' + book_list[book_num_4].url + '" target="_blank" </a>')
			$('#book_4_title').text(book_list[book_num_4].title + '')
		}

		function shuffle(a) {
			for (let i = a.length - 1; i > 0; i--) {
				const j = Math.floor(Math.random() * (i + 1));
				[a[i], a[j]] = [a[j], a[i]];
			}
			return a;
		}